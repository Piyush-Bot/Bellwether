style.
    body {
        background-repeat: repeat;
        background: #f9f9f9;
        color: #354558;
    }
    p{
        font-family: "Poppins", sans-serif;
        margin-top:20px;
        margin-bottom:20px;
        display:inline-block;
    }
    a{
        text-decoration:none;
    }
    .wrapper {
        width: 100%;
        height: 100%;
        float: left;
        position: fixed;
        background: #fff;
        top: 0;
        left: 0;
        text-align: center;
    }
    .content-sec {
        max-width: 500px;
        width: 90%;
        margin: 0 auto;
    }
    .out {
        width: 100%;
        height: 100%;
        float: none;
        display: table;
    }
    .in {
        width: 100%;
        height: 100%;
        float: none;
        display: table-cell;
        vertical-align: middle;
        text-align: center;
    }
    .st1 {
        font-size: 83.0285px;
    }
    svg{
        width: 100%;
        max-width:450px;
        height: auto;
        text-align: center;
        fill: #43c4c4;
        padding-bottom:20px;
    }
    path#XMLID_5_ {
        fill: #43c4c4;
        filter: url(#blurFilter4);
    }
    path#XMLID_11_ ,path#XMLID_2_ {
        fill: #43c4c4;
    }
    .circle{
        animation: out 2s infinite ease-out;
        fill: #43c4c4;
    }
    .btn{
        font-family: "Poppins", sans-serif;
        font-weight: 300;
        text-transform: uppercase;
        overflow: hidden;
        border-radius: 10em;
        font-size: 16px;
        padding:.5rem 1.5rem;
        margin: 6px;
        white-space: normal!important;
        box-shadow:0 2px 5px 0 rgb(0 0 0 / 16%), 0 2px 10px 0 rgb(0 0 0 / 12%);
        background:#43c4c4;
        color:#fff;
        display:inline-block;
    }
    .btn:hover{
        background:#1aa29c;
    }
    #container{
        text-align:center;
    }
    .message{
        color:#43c4c4;
        font-size:23px;
        font-family: "Poppins", sans-serif;
    }
    .message:after{
        content:"]";
    }
    .message:before{
        content:"[";
    }
    .message:after, .message:before {
        color: #43c4c4;
        font-size: 25px;
        -webkit-animation-name: opacity;
        -webkit-animation-duration: 2s;
        -webkit-animation-iteration-count: infinite;
        animation-name: opacity;
        animation-duration: 2s;
        animation-iteration-count: infinite;
        margin:0 25px;
    }
    .bottom-cnt{
        width:100%;
        float:left;
    }
    @-webkit-keyframes opacity {
        0%, 100% {
            opacity: 0;
        }
        50% {
            opacity: 1;
        }
    }
    @keyframes opacity {
        0%, 100% {
            opacity: 0;
        }
        50% {
            opacity: 1;
        }
    }
    @keyframes out {
        0% {r:1;  opacity: 0.9 ;}
        25%{r:5;  opacity: 0.3 ;}
        50%{r:10; opacity: 0.2 ;}
        75%{r:15;opacity:0.1;}
        100% {r:20;opacity:0;}
    }
